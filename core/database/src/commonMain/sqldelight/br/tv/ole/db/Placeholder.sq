CREATE TABLE Placeholder (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL UNIQUE
);

selectAll:
SELECT * FROM Placeholder;

selectById:
SELECT * FROM Placeholder WHERE id = ?;

selectByName:
SELECT * FROM Placeholder WHERE name = ?;

insertPlaceholder:
INSERT OR IGNORE INTO Placeholder(name)
VALUES (?);

deleteAll:
DELETE FROM Placeholder;
